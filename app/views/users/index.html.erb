<div class="container">
  <div class="row">
<%= render 'common/header' %>
<p class="notice"><%= flash[:notice] %></p>
<%= render 'common/userinfo' %>
<!--%= render 'books/new' %-->

<!-- user一覧 -->
<div class="col-xs-9">
<table>
  <tr>
    <th>images</th>
    <th>name</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td>
      <%= link_to user_path(user.id ) do %>
      <%= attachment_image_tag user, :profile_image, size: "60x60", fallback: "no_image.jpg" %>
      <% end %>
    </td>
  	<td><%= user.name %></td>
    <td><%= link_to "Show", user_path(user.id) %></td>
  </tr>
  <% end %>
</table>
</div>
</div>
</div>